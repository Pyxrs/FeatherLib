{
  "schemaVersion": 1,
  "id": "featherlib",
  "version": "${version}",

  "name": "FeatherLib",
  "description": "Some helpful code for my mods",
  "authors": [
    "SimplyCmd"
  ],
  "contact": {
    "homepage": "https://simplycmd.github.io/FeatherLib",
    "sources": "https://github.com/SimplyCmd/FeatherLib"
  },

  "license": "MIT",
  "icon": "assets/featherlib/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.simplycmd.featherlib.scheduler.ServerScheduler",
      "com.simplycmd.featherlib.registry.RegistryHandler",
      "com.simplycmd.featherlib.util.Util"
    ],
    "client": [
      "com.simplycmd.featherlib.scheduler.ClientScheduler",
      "com.simplycmd.featherlib.example.ExampleScheduler"
    ],
    "registry": [
      "com.simplycmd.featherlib.example.ExampleRegistry"
    ]
  },
  "mixins": [
    "featherlib.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.19.x",
    "java": ">=17",
    "better_runtime_resource_pack": "*"
  },
  "suggests": {
  }
}
